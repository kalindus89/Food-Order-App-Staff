<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.foodorderappstaff">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".send_notification_all_subscribers.SendNotificationsAllActivity"
            android:exported="true" />
        <activity
            android:name=".updateBanners.UpdateBannerActivity"
            android:exported="true" />
        <activity
            android:name=".notification_manager.ActivityTestNotification"
            android:exported="true" />
        <activity
            android:name=".WelcomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".singin_signup.SignInActivity"
            android:exported="true" />
        <activity
            android:name=".all_foods_home.HomeActivity"
            android:exported="true" />
        <activity
            android:name=".specific_foods_list.FoodsListActivity"
            android:exported="true" />
        <activity
            android:name=".all_order_status_and_history.AllOrderStatusActivity"
            android:exported="true" />
        <activity
            android:name=".all_order_status_and_history.CurrentJobOrdersActivity"
            android:exported="true" />
        <activity
            android:name=".all_order_status_and_history.HistoryJobOrdersActivity"
            android:exported="true" />
        <activity
            android:name=".view_order_foods.ViewOrderFoods"
            android:exported="true" />

        <service
            android:name=".notification_manager.ListenAllOrdersService"
            android:enabled="true" />
    </application>

</manifest>